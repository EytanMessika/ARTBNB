<%= simple_form_for [@artwork] do |f| %>
  <%= f.input :name %>
  <%= f.input :artist_name %>
  <%= f.input :category, :collection => ["Painting", "Scultpure", "Photography", "Icon", "Drawing"] %>
  <%= f.input :dimensions %>
  <%= f.input :price %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>
